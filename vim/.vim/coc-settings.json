{
  "coc.preferences.watchmanPath": "DOES_NOT_EXIST__SETTING_TO_DISABLE",
  "suggest.timeout": 5000,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "●",
  "eslint.run": "onSave",
  "jest.terminalPosition": "right",
  "jest.forceExit": true,
  "jest.customFlags": ["--coverage=false"],
  "prettier.requireConfig": true,
  "typescript.format.enabled": false,
  "javascript.format.enabled": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "rust"
  ],
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.inlayHints.chainingHintsSeparator": "▶︎ ",
  "rust-analyzer.inlayHints.typeHintsSeparator": "▶︎ "
}
